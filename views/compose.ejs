<%- include('partials/header') -%>
<h1> Compose </h1>
<form action="/compose" method="post">
  <div class="form-group">
    <label>Title</label>
    <input type="text" class="form-control" name="postitle">
    <label>Post</label>
    <textarea name="postcontent" class="form-control" rows="5" cols="30"></textarea>
